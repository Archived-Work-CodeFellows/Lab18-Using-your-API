@model List<Lab18ConsumeApi.Models.ToDoList>

@foreach (var list in Model)
{
    <a asp-controller="ToDoList" asp-action="Details" asp-route-id="@list.ID"><p>@list.Name</p></a>

}
<form method="get" asp-controller="ToDoList" asp-action="Create">
    <button type="submit">Add new list</button>
</form>
<form method="get" asp-controller="ToDo" asp-action="Index">
    <button type="submit">View Current tasks</button>
</form>